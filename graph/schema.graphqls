type Query{
	ordersForRegion(
		region_id : Int
		order_type : Ordertype = all
		type_id : Int
	) : [Order]
	systemById(id : Int) : System
	stationById(id : Int) : Station
	planetById(id : Int) : Planet
	corporationById(id : Int) : Corporation
	factionByID(id : Int) : Faction

}
enum Ordertype {
	buy
	sell
	all
}
type Order {
	duration : Int
	is_buy_order : Boolean
	issued : String
	location : Station
	location_id: Int
	min_volume : Int
	order_id : Int!
	price : Float
	range : Range
	system : System
	system_id: Int
	item_type : Item_type
	type_id : Int
	volume_remain : Int
	volume_total : Int
}

