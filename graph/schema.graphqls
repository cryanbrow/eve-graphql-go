	published : Boolean
	radius : Float
	volume : Float
}

type DogmaAttribute{
	attribute : DogmaAttributeDetail
	value : Float
}

type DogmaAttributeDetail{
	attributeId : ID
	defaultValue : Float
	description : String
	displayName : String
